{
    "name": "dru-id/laravel-themes",
    "description": "Themes and Assets Manager for Laravel based on Assetic and Shipu Themevel",
    "authors": [
        {
            "name": "idominguez",
            "email": "israel.dominguez.garcia@gmail.com"
        }
    ],
    "type": "library",
    "version": "dev-master",
    "require": {
        "kriswallsmith/assetic": "1.*",
        "leafo/lessphp": "^0.3.7",
        "leafo/scssphp": "~0.1",
        "meenie/javascript-packer": "^1.1",
        "mrclay/minify": "<2.3",
        "natxet/cssmin": "3.0.6",
        "ptachoire/cssembed": "~1.0"
    },
    "require-dev": {
        "illuminate/support": "~5.5.0",
        "illuminate/validation": "~5.5.0",
        "illuminate/view": "~5.5.0"
    },
    "autoload": {
        "psr-4": {
            "Genetsis\\Themes\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Genetsis\\Themes\\ThemeServiceProvider"
            ],
            "aliases": {
                "Asset": "Genetsis\\Themes\\Facades\\AssetFacade",
                "Theme": "Genetsis\\Themes\\Facades\\ThemeFacade"
            }
        }
    }
}
